{
  "adb":{
    "terminal_encoding": "utf-8",
    "adb_path": "adb"
  },
  "logging_config": {
    "version": 1,
    "formatters": {
      "simple": {
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
      }
    },
    "handlers": {
      "file_handler": {
        "class": "logging.FileHandler",
        "level": "DEBUG",
        "formatter": "simple",
        "filename": "application.log"
      },
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "simple"
      }
    },
    "root": {
      "level": "DEBUG",
      "handlers": ["file_handler", "console"]
    }
  }
}